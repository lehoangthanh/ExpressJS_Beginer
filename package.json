	"name": "expressjs_beginer",
	"version": "1.0.0",
	"main": "./bin/www.js",
	"scripts": {
		"test": "mocha --require @babel/register",
		"start": "npm run build && nodemon dist/bin/www.js",
		"build": "babel ./bin/www.js -d dist",
		"format:check": "prettier --check .",
		"format:write": "prettier --write .",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"prepare": "husky install"
	},
	"dependencies": {
		"body-parser": "^1.20.0",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.0.0",
		"express": "^4.18.0",
		"lodash": "^4.17.21",
		"module-alias": "^2.2.2",
		"mongoose": "^6.3.2",
		"npm-run-all": "^4.1.5"
	},
	"devDependencies": {
		"@babel/cli": "^7.17.10",
		"@babel/core": "^7.17.10",
		"@babel/node": "^7.17.10",
		"@babel/preset-env": "^7.17.10",
		"@babel/register": "^7.17.7",
		"chai": "^4.3.6",
		"chai-http": "^4.3.0",
		"eslint": "^8.14.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^7.0.0",
		"lint-staged": "^12.4.1",
		"mocha": "^10.0.0",
		"nodemon": "^2.0.15",
		"prettier": "^2.6.2"
	},
	"lint-staged": {
		"src|tests/**/*.js": [
			"npm run lint"
		],
		"*.js": "eslint"
	},
	"_moduleAliases": {
		"@root": ".",
		"@deep": "src/some/very/deep/directory/or/file",
		"@app": "./app.js",
		"@routes": "src/routes",
		"@controllers": "src/controllers"
	}
}
